[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[ dn ]
C  = US
ST = California
L  = San Francisco
O  = MyOrganization
OU = MyUnit
CN = webhook-service.default-system.svc

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1   = webhook-service.default-system.svc
DNS.2   = webhook-service.default-system.svc.cluster.local
